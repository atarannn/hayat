
extends ../layout/main
block variable
  -
    var title_page = 'home'
    var id_page = 'home-page'
    var class_page = 'home'
    var slider = [
      {
        img: 'infra/1.jpg',
        num: '01',
        text: 'Большой открытый бассейн с горками, детский бассейн, внутренний бассейн с подогревом'
      },
      {
        img: 'infra/2.jpg',
        num: '02',
        text: 'СПА: сауна и хамам, комнаты отдыха, тренажёрный зал'
      },
      {
        img: 'infra/3.jpg',
        num: '03',
        text: 'Барбекю, беседки для отдыха, детская игровая площадка'
      },
      {
        img: 'infra/4.jpg',
        num: '04',
        text: 'Лифт, открытая парковка, интернет Wi-Fi, видеонаблюдение 24/7'
      },
    ]
    var slider2 = [
      {
        img: 'flats/1.jpg',
        num: '01',
        text: 'Cтальные двери, домофон с видеосистемой\n'
      },
      {
        img: 'flats/2.jpg',
        num: '02',
        text: 'Подвесные потолки и точечное скрытое освещение'
      },
      {
        img: 'flats/3.jpg',
        num: '03',
        text: 'Моющаяся краска на стенах, пол - из гранита первого класса'
      },
      {
        img: 'flats/4.jpg',
        num: '04',
        text: 'ПВХ окна и двери на балкон с двойным остеклением и шумоизоляцией'
      },
      {
        img: 'flats/5.jpg',
        num: '05',
        text: 'Полностью укомплектованная ванная комната'
      },
      {
        img: 'flats/6.jpg',
        num: '06',
        text: 'Кухонный гарнитур\n'
      },
